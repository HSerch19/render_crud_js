<%- include('header') -%>

<div class="container mt-5">
    <h1>Lista de Categorias</h1>
    <table class="table table-bordered table-hover">
        <thead>
            <tr>
                <th>ID</th>
                <th>Nombre Categoria</th>
            </tr>
        </thead>
        <tbody>
                <% for (const Categorias of modelo) { %>
                <tr> 
                    <td><%= Categorias.id %></td>
                    <td><%= Categorias.categoria %></td>
                    </tr>
            <% } %>
        </tbody>
    </table>
</div>